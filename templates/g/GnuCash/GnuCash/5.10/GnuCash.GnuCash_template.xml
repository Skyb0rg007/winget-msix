    <Installer
        Path="C:\Users\skyle\Downloads\gnucash-5.10.setup.exe"
        Arguments="/VERYSILENT /NORESTART"
        InstallLocation="C:\Program Files (x86)\gnucash" />

    <PackageInformation
        PackageName="GnuCash.GnuCash"
        PackageDisplayName="GnuCash"
        PublisherName="CN=skye@soss.website"
